#include "main.h"
#include <string.h>
/**
 * @_strcat - function that concatenates two strings
 * @dest: first string
 * @src: second string
 * 
 * Return - Always (0)
 */

char *_strcat(char *dest, char *src)
{
	char destination[] = "dest ";
	char source [] = "src";

	_strcat(destination,source);
	_putchar("%s", destination);

		_putchar('\n');
		return (0);
}
